06
#include <stdio.h>
int main(void)
{
	char *ptr;
	int nr;
	
	ptr = "";
	nr = ft_str_is_printable(ptr);
	printf("%d", nr);
	
	return (0);
}

********************************************************************************************************************
07

#include <stdio.h>
int main(void)
{
	char teste[] = {"Teste"};
	//char texto_dest[7];
	printf("Texto original: %s\n", teste);
	ft_strupcase(teste);
	printf("Texto final: %s\n", teste);
	return (0);	
}

********************************************************************************************************************
08
#include <stdio.h>
int main(void)
{
	char teste[] = {"Teste"};
	//char texto_dest[7];
	printf("Texto original: %s\n", teste);
	ft_strupcase(teste);
	printf("Texto final: %s\n", teste);
	return (0);	
}